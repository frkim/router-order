{
  "azureLogicAppsStandard.projectPath": "logicapp",
  "azureFunctions.deploySubpath": "logicapp",
  "azureFunctions.projectLanguage": "Workflow",
  "azureFunctions.projectRuntime": "~4",
  "azureFunctions.preDeployTask": "",
  "debug.internalConsoleOptions": "neverOpen",
  "files.associations": {
    "*.json": "json"
  },
  "json.schemas": [
    {
      "fileMatch": [
        "/logicapp/**/workflow.json"
      ],
      "url": "https://schema.management.azure.com/providers/Microsoft.Logic/schemas/2016-06-01/workflowdefinition.json#"
    }
  ],
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "redhat.vscode-yaml",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  }
}
